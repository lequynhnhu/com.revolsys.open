  <name>PostGIS Reader</name>
  <groupId>com.revolsys.open</groupId>
  <artifactId>com.revolsys.open.gis.postgis</artifactId>
  <version>2011.11.16-2.RELEASE</version>
  <url>http://www.revolsys.com</url>
  <parent>
